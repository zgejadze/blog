<header>
    <ul id="navbar">
        <li><a class="navbar-links" href="/">Home</a></li>
        <% if(!locals.isAuth){ %> 
        <li><a class="navbar-links" href="/signup">Signup</a></li>
        <li><a class="navbar-links" href="/login">Login</a></li>
        <% } %>
        <% if(locals.isAuth) { %>  
        <li><a class="navbar-links" href="/admin">Admin</a></li>
        <form action="/logout" method="POST">
           <input type="hidden" value="<%= csrfToken %>" name="_csrf">
            <li><button>Logout</button></li>
        </form>
        <% } %> 
    </ul>
</header>